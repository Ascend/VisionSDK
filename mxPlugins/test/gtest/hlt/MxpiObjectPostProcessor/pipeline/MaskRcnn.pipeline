{
    "maskrcnn": {
        "stream_config": {
            "deviceId": "0"
        },
        "appsrc0": {
            "props": {
                "blocksize": "409600"
            },
            "factory": "appsrc",
            "next": "mxpi_imagedecoder0"
        },
		"mxpi_imagedecoder0": {
            "props": {
                "cvProcessor":"opencv"
            },
            "factory": "mxpi_imagedecoder",
            "next": "mxpi_imageresize0"
        },
        "mxpi_imageresize0": {
        "props": {
            "cvProcessor":"opencv"
        },
            "factory": "mxpi_imageresize",
            "next": "mxpi_tensorinfer0" 
        },
        "mxpi_tensorinfer0": {
            "props": {
                "modelPath": "../models/maskrcnn/maskrcnn.om"
            },
            "factory": "mxpi_tensorinfer",
            "next": "mxpi_objectpostprocess0"
        },
		"mxpi_objectpostprocess0":{
	        "props":{
	        "dataSource": "mxpi_tensorinfer0",
                "postProcessConfigPath": "../models/maskrcnn/maskrcnn.cfg",
                "labelPath": "../models/maskrcnn/maskrcnn.names",
                "postProcessLibPath": "libmaskrcnnmindsporepost.so"
	        },
            "factory":"mxpi_objectpostprocessor",
            "next":"mxpi_dumpdata0"
        },
		"mxpi_dumpdata0": {
			"props": {
				"requiredMetaDataKeys": "mxpi_objectpostprocess0",
				"location": "./output/maskrcnnActual.output"
			},
            "factory": "mxpi_dumpdata",
            "next": "appsink0"
        },
		"appsink0": {
			"factory": "appsink"
		}
    }
}
